@model List<Rating>
@{
    ViewData["Title"] = "Course Ratings";
}
<h2>Course Ratings</h2>
<br>
@foreach (var rating in Model)
{
    <div class="card mb-3">
        <div class="card-body">
            <h5>Rating Id: @rating.RatingId</h5>
            <p>Course Name: @rating.CourseName</p>
            <p>Rating Value: @rating.RatingValue/5&nbsp;<i class="bi bi-star-fill"></i>
            <p>Rating Description: @rating.RatingDescription</p>
            <p><small class="text-muted">Rating Time: @rating.RatingTime</small></p>
        </div>
    </div>
}